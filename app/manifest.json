{
  "name": "__MSG_appName__",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "author": "https://github.com/mchugh19/zabbix-vue",
  "icons": {
    "128": "images/logo.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "background",
    "notifications",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": {
      "38": "images/unconfigured.png"
    },
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "images/logo.png"
  ]
}
